{
	"name": "npm-browser",
	"displayName": "NPM Browser",
	"description": "",
	"publisher": "dennisvhest",
	"repository": "https://github.com/DennisvHest/vscode-npm-browser",
	"version": "1.0.0-beta",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:dependencies"
	],
	"main": "./apps/extension/src/main.js",
	"enableProposedApi": true,
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "npm-browser",
					"title": "NPM Browser",
					"icon": "./apps/extension/assets/icon.svg"
				}
			]
		},
		"views": {
			"npm-browser": [
				{
					"id": "dependencies",
					"name": "Dependencies"
				}
			]
		}
	}
}